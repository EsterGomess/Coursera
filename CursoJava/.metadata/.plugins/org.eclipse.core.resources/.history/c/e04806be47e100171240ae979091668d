import java.util.HashMap;
import java.util.Map;
public class CarrinhoDeCompra {

	Map<Produto,Integer> itenscompra;
	
	public void adicionaProduto(Produto produto,int quantidade){
		itenscompra = new HashMap<Produto,Integer>();
		for (int i=0; i < itenscompra.size(); i++){
			if(itenscompra.containsKey(ingrediente)){
				int valor =itenscompra.get(ingrediente)+1;
				itenscompra.put(ingrediente, valor);
			}else{
				itenscompra.put(ingrediente, 1);
			}	
			
		}
	}
}
